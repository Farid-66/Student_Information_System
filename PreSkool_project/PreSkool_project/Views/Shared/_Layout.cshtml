@inject UserManager<CustomUser> _userManager
@inject SignInManager<CustomUser> _signInManager
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="~/Image/logo-small.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
        integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous" />
    <link rel="stylesheet" href="~/css/style.css" />
    <title>PreSkool</title>
</head>

<body>

    <!-- Header Start -->
    <header>

        <div class="header_left">
            <div class="header_mobile_btn">
                <a href="#" class="mobile_btn"><i class="fas fa-bars"></i></a>
            </div>
            <a asp-controller="admin" asp-action="index">
                <img src="~/Image/logo-small.png" class="logo" alt="">
            </a>
            <div></div>
        </div>
        <div class="header_btn">
            <a href="#" class="header_link"><i class="fas fa-align-left"></i></a>
        </div>
        <div class="header_search">
            <form action="">
                <input type="search" placeholder="Search here">
                <button type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
            </form>
        </div>
        <div class="header_user_menu">
            <a href="#" id="drop_btn">
                <p><b>@_userManager.GetUserName(User)</b></p>
                <i class="fa-solid fa-angle-down"></i>
            </a>
            <div class="dropdown_user">
                <div class="user_header">
                    <div class="user_text">
                        <p><b style="font-size: 15px;">@_userManager.GetUserName(User)</b></p>
                    </div>
                </div>
                <ul>
                    @*<li><a href="profil.html" class="dropdown_item">My Profile</a></li>*@
                    <li><a asp-controller="Account" asp-action="logout" class="dropdown_item">Logout</a></li>
                </ul>
            </div>
        </div>


    </header>
    <!-- Header End -->

    <!-- Sidebar start -->
    <div class="sidebar">
        <div class="sidebar-inner">
            <div class="sidebar-menu">
                <ul>
                    <li class="sidmenu-item"><a asp-controller="admin" asp-action="index" class="sidmenu_link_All"><i class="fa-solid fa-house"></i>
                            <span>Dashboard</span></a>
                    </li>
                    <li class="sidmenu-title">
                        <h6>Users</h6>
                    </li>
                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All">
                            <i class="fa-solid fa-user"></i>
                            <span>User</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="account" asp-action="users">User List</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a  class="sidmenu-link sidmenu_link_All">
                            <i class="fa-solid fa-users"></i>
                            <span>Role</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="account" asp-action="Roles">Role List</a></li>
                            <li><a asp-controller="account" asp-action="rolecreate">Role Add</a></li>
                        </ul>
                    </li>

                    <li class="sidmenu-title">
                        <h6>Main Menu</h6>
                    </li>
                    <li class="sidmenu-item"><a class="sidmenu-link sidmenu_link_All"><i
                                class="fas fa-user-graduate"></i>
                            <span>Students</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="student" asp-action="index">Student List</a></li>
                            <li><a asp-controller="student" asp-action="create">Student Add</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All"><i
                                class="fas fa-chalkboard-teacher"></i>
                            <span>Teachers</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="Teacher" asp-action="index">Teacher List</a></li>
                            <li><a asp-controller="Teacher" asp-action="create">Teacher Add</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All"><i
                                class="fas fa-building"></i>
                            <span>Departments</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="department" asp-action="index">Department List</a></li>
                            <li><a asp-controller="department" asp-action="create">Department Add</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All"><i
                                class="fas fa-book-reader"></i>
                            <span>Subjects</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="Subject" asp-action="index">Subject List</a></li>
                            <li><a asp-controller="Subject" asp-action="create">Subject Add</a></li>
                        </ul>
                    </li>

                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All">
                        <i class="fa-solid fa-section"></i>
                            <span>Section</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="Sections" asp-action="index">Section List</a></li>
                            <li><a asp-controller="Sections" asp-action="create">Section Add</a></li>
                        </ul>
                    </li>

                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All">
                        <i class="fas fa-chalkboard"></i>
                            <span>Class</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="Classes" asp-action="Index">Class List</a></li>
                            <li><a asp-controller="Classes" asp-action="Create">Class Add</a></li>
                            <li><a asp-controller="ClassToSubjects" asp-action="index">Class To Subjects</a></li>
                        </ul>
                    </li>

                    <li class="sidmenu-title">
                        <h6>Management</h6>
                    </li>

                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All"><i
                                class="fas fa-file-invoice-dollar"></i>
                            <span>Accounts</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="annual" asp-action="index">Annual</a></li>
                            <li><a asp-controller="Expensestype" asp-action="index">Expenses Type</a></li>
                            <li><a asp-controller="Expenses" asp-action="index">Expenses</a></li>
                            <li><a asp-controller="Salary" asp-action="index" >Salary</a></li>
                            <li><a asp-controller="annual" asp-action="create">Add Annual</a></li>
                            <li><a asp-controller="Expensestype" asp-action="create">Add Expenses Type</a></li>
                            <li><a asp-controller="Expenses" asp-action="create">Add Expenses</a></li>
                            <li><a asp-controller="Salary" asp-action="create" >Add Salary</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a href="#" class="sidmenu-link sidmenu_link_All">
                            <i class="fa-solid fa-holly-berry"></i>
                            <span>Holiday</span>
                            <span class="fa-solid fa-angle-right"></span></a>
                        <ul class="sublimenu">
                            <li><a asp-controller="holidaytypes" asp-action="index">Holiday Type</a></li>
                            <li><a asp-controller="holidays" asp-action="index">Holiday</a></li>
                        </ul>
                    </li>
                    <li class="sidmenu-item"><a asp-controller="exam" asp-action="index" class="sidmenu_link_All"><i class="fas fa-clipboard-list"></i>
                            <span>Exam List</span></a></li>
                    <li class="sidmenu-item"><a asp-controller="event" asp-action="index" class="sidmenu_link_All"><i class="fas fa-calendar-day"></i>
                            <span>Events</span></a>
                    </li>
                    <li class="sidmenu-item"><a asp-controller="Library" asp-action="index" class="sidmenu_link_All"><i class="fas fa-book"></i>
                            <span>Library</span></a></li>
                    <li class="sidmenu-item mt-5"></li>
                </ul>
            </div>
        </div>

    </div>
    <div class="overlay"></div>

    <!-- Sidebar End -->

    <!-- Main Page Start -->

    @RenderBody()

    <!-- Main Page End -->

    @*<footer class="mt-5" style="border-top: 1px solid #ddd;padding: 1.875rem 1.875rem;position:relative;bottom:-100px;">
        <p style="margin-bottom:0px;font-weight:bold;" class="text-end">Copyright © 2022 Preskool</p>
    </footer>*@


    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
        integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.9.2/dist/umd/popper.min.js"
        integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
        integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
        crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
     <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.4/dist/sweetalert2.all.min.js"></script>

     <script>
         $(document).ready(function(){
             //==============# Active Link #====================
            $(".sidebar-inner").find('[href="' + window.location.pathname + '"]').parent().addClass("active-item");
            //$(".active-item").parent().addClass("active-sublimenu");
            //=================================================
         })
     </script>

    <script src="~/js/script.js"></script>
</body>

</html>